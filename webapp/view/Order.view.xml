<mvc:View
    xmlns:launchpad="sap.ushell.ui.launchpad"
    controllerName="com.collak.home.central.central.controller.Order"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:form="sap.ui.layout.form"
    xmlns:l="sap.ui.layout"
    xmlns:core="sap.ui.core"
>
    <Page showHeader="false">
        <content>
            <VBox>
                <f:ShellBar
                    id="shellbar_main3"
                    title="{i18n>orders}"
                    secondTitle="{i18n>shortDescription}"
                    homeIcon="./resources/sap/ui/documentation/sdk/images/logo_sap.png"
                    showCopilot="true"
                    showSearch="false"
                    showNotifications="true"
                    notificationsNumber="2"
                    homeIconPressed="handleHomeIconPress"
                >
                    <f:menu>
                        <Menu id="menu_main3">
                            <MenuItem
                                id="menu_item_123"
                                text="Dashboard"
                                icon="sap-icon://home"
                            />
                            <MenuItem
                                id="menu_item_223"
                                text="ObjednÃ¡vky"
                                icon="sap-icon://list"
                            />
                        </Menu>
                    </f:menu>
                    <f:profile>
                        <Avatar
                            press="handleAvatarPress"
                            id="avatarId23"
                            initials="UI"
                        />
                    </f:profile>
                </f:ShellBar>
                <ObjectHeader 
                    intro="{Orders>ShipperName}"
                    title="{Orders>ProductName}"
                >
                </ObjectHeader>
            </VBox>
        </content>
    </Page>
</mvc:View>
