<mvc:View
    xmlns:launchpad="sap.ushell.ui.launchpad"
    controllerName="com.collak.home.central.central.controller.Order"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:form="sap.ui.layout.form"
    xmlns:l="sap.ui.layout"
    xmlns:core="sap.ui.core"
>
    <Page showHeader="false">
        <content>
            <VBox>
                <core:Fragment fragmentName="com.collak.home.central.central.view.fragment.ShellBar" type="XML"/>
                <ObjectHeader 
                    intro="{Orders>ShipperName}"
                    title="{Orders>ProductName}"
                >
                </ObjectHeader>
            </VBox>
        </content>
    </Page>
</mvc:View>
